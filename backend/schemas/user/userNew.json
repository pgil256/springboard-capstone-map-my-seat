{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "email",
        "title",
        "isAdmin",
        "firstName",
        "lastName"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "string"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "string"
            ]
        },
        "isAdmin": {
            "type": "boolean",
            "default": false,
            "title": "The isAdmin Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "username": "string",
        "password": "string",
        "isAdmin": true
    }]
}